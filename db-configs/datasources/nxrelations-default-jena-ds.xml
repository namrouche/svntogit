<?xml version="1.0" encoding="UTF-8"?>

<datasources>
  <local-tx-datasource>
    <jndi-name>nxrelations-default-jena</jndi-name>
    <connection-url>
      @jdbcUrl@
    </connection-url>
    <driver-class>@jdbcDriver@</driver-class>
    <user-name>@jdbcUser@</user-name>
    <password>@jdbcPassword@</password>
  </local-tx-datasource>

  <mbean code="org.jboss.jdbc.HypersonicDatabase"
    name="jboss:service=Hypersonic,database=nxrelations-default-jena">
    <attribute name="Database">nxrelations-default-jena</attribute>
    <attribute name="InProcessMode">true</attribute>
  </mbean>
</datasources>

